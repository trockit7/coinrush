generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client-main"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL_MAIN")
}

model Token {
  id           Int      @id @default(autoincrement())
  token_addr   String   @unique
  pool_addr    String   @unique
  name         String
  symbol       String
  image_url    String?
  description  String?
  website      String?
  telegram     String?
  twitter      String?
  created_by   String?
  created_at   DateTime @default(now()) // ðŸ‘ˆ change from BigInt to DateTime
}
